{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@rnbridge/web#tsc": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["^build", "@rnbridge/web#tsc"],
      "outputs": ["dist/**"]
    },
    "lint": { "cache": false },
    "test:type": {
      "cache": false
    }
  }
}
